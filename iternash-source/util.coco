import sys


Str = (str, bytes)


def printret(obj) =
    """Print then return _obj_."""
    print(obj)
    obj


def clip(x, m=sys.float_info.epsilon, M=1-sys.float_info.epsilon):
    """Clip x into [m, M] (defaults to [eps, 1-eps])."""
    if x <= m:
        return m
    elif x >= M:
        return M
    else:
        return x


def real(x) =
    """Get only the real part of x."""
    x.real if isinstance(x, complex) else x
